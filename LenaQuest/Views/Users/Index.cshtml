@model LenaQuest.ViewModels.UserSelectionViewModel

    <partial name="ExportUsers">
<h2>Please select users</h2>

<form asp-action="SendRequest" method="post">
    <table class="table text-center">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>City</th>
            <th>QuestExpirience</th>
            <th>Choose user</th>
        </tr>

        @Html.EditorFor(model => model.Users)
    </table>
    <div class="form-group">
        <button type="submit" class="btn btn-success">
            Send details
        </button>
    </div>
</form>

