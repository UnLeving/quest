@model LenaQuest.Models.UserProfile

@{
    ViewData["Title"] = "Index";
}
<div class="form-group">
    <label asp-for="FirstName"></label>
    <input asp-for="FirstName" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="SecondName"></label>
    <input asp-for="SecondName" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Age"></label>
    <input asp-for="Age" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="PhoneNumber"></label>
    <input asp-for="PhoneNumber" class="form-control" type="number" />
    <span asp-validation-for="PhoneNumber" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="City"></label>
    <input asp-for="City" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="QuestExpiriencs"></label>
    <input asp-for="QuestExpiriencs" class="form-control" type="number" />
</div>
